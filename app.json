{
  "expo": {
    "name": "BullionDesk",
    "slug": "BullionDesk",
    "sdkVersion": "51.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "duration": 2000
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png"
      },
      "package": "com.sonitech.bulliondesk",
      "permissions": [
        "POST_NOTIFICATIONS",
        "SCHEDULE_EXACT_ALARM",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/app-icon.png",
          "color": "#ffffff",
          "androidMode": "default",
          "androidCollapsedTitle": "BullionDesk Reminders"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f3051b11-36d0-4cbf-acae-31058fde66f4"
      }
    },
    "owner": "imaaryan3563"
  }
}
