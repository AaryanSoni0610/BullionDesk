{
  "expo": {
    "name": "BullionDesk",
    "slug": "BullionDesk",
    "sdkVersion": "51.0.0",
    "version": "0.0.7",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png"
      },
      "package": "com.sonitech.bulliondesk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "f3051b11-36d0-4cbf-acae-31058fde66f4"
      }
    },
    "owner": "imaaryan3563"
  }
}
